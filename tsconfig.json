{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/units/board/write/BoardWrite.Container.js",
    "src/components/commons/validationFile",
    "pages/main/starbucks/index.tsx",
    "pages/main/starbucks/menuItem/index.tsx",
    "pages/main/starbucks/coffeeList/index.tsx",
    "pages/main/starbucks/coffeeItem/index.tsx",
    "pages/main/index.js"
  ],
  "exclude": ["node_modules"]
}
